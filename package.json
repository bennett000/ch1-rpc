{
  "name": "js-rpc",
  "description": "JavaScript Remote Procedure Call (RPC)",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/bennett000/js-rpc.git"
  },
  "scripts": {
    "clean": "npm run clean:coverage && npm run clean:dist && npm run clean:lib",
    "clean:coverage": "rimraf ./coverage",
    "clean:dist": "rimraf ./dist",
    "clear:lib": "rimraf ./lib",
    "lint": "npm run lint-ts &&  npm run lint-js",
    "lint-js": "eslint . ",
    "lint-ts": "tslint ./src/**/*.ts",
    "pretest": "rimraf coverage/ && npm run lint",
    "postinstall": "npm run typings",
    "test": "karma start --singleRun true",
    "test:watch": "karma start --singleRun false --restartOnFileChange true",
    "tsc": "tsc --declaration --outDir lib",
    "tsc:watch": "tsc -w --declaration --outDir lib",
    "typedoc": "typedoc --out docs-generated ./src/index.ts",
    "typings": "typings install"
  },
  "dependencies": {
    "es6-promise": "^3.2.1"
  },
  "peerDependencies": {
    "rxjs-es": "^5.0.0-beta.9"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^2.0.0-rc.18",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.0.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "connect": "~2.16.2",
    "eslint": "^2.12.0",
    "eslint-loader": "^1.3.0",
    "express": "~4.2.0",
    "istanbul": "^0.4.4",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "jasmine-node": "~1.14.2",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "requirejs": "~2.1.11",
    "rimraf": "^2.5.2",
    "socket.io": "~0.9.16",
    "ts-helpers": "^1.1.1",
    "tslint": "^3.11.0",
    "typedoc": "^0.4.4",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "webpack": "^1.13.1"
  }
}
